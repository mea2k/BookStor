<%- include('../../layouts/layout_start', {title}) %>


<h1>
    <%= title %>
</h1>


<div class="row">
    <div class="col-sm-8">
        <div class="card">
            <div class="card-body">

                <h5 class="card-title"><%= user.login %></h5>
                <h6><%= user.name %></h6>
                <p class="card-text"><%= user.email %></p>

            </div>
        </div>
    </div>
</div>




<%- include('../../layouts/layout_end') %>

